        "@supabase/supabase-js": "latest",
        "dotenv": "^16.3.1",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.0",
        "@typescript-eslint/eslint-plugin": "^8.56.0",
        "@typescript-eslint/parser": "^8.56.0",
        "eslint": "^9.39.3",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
